{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": [
        "./src/components/*"
      ],
      "@/layouts/*": [
        "./src/layouts/*"
      ],
      "@/pages/*": [
        "./src/components/pages/*"
      ],
      "@/icons/*": [
        "./src/components/icons/*"
      ],
      "@/*": [
        "./src/*"
      ]
    }
  },
}
